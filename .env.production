# Production Environment Configuration
NODE_ENV=production

# Production optimizations
WEBPACK_MINIMIZE=true
WEBPACK_TREE_SHAKING=true
CSS_MINIMIZE=true

# Performance settings
WEBPACK_PERFORMANCE_HINTS=warning
MAX_ENTRYPOINT_SIZE=1048576
MAX_ASSET_SIZE=524288

# Security settings
CSP_ENABLED=true
ASAR_ENABLED=true

# Build settings
SOURCE_MAPS=true
BUNDLE_ANALYZER=false